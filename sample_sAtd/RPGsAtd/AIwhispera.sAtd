import json
import os

def load_character_cards(directory):
    """
    指定されたディレクトリ内のV2カード（JSON形式）を読み込み、
    キャラクターデータを格納した辞書を返します。
    """
    characters = {}
    for filename in os.listdir(directory):
        if filename.endswith(".json"):
            filepath = os.path.join(directory, filename)
            try:
                with open(filepath, "r", encoding="utf-8") as f: #文字コードをUTF-8に指定
                    character_data = json.load(f)
                    character_id = character_data.get("name") # キャラクター名をIDとして使用
                    if character_id:
                        characters[character_id] = character_data
                    else:
                        print(f"Error: Character name not found in {filename}")
            except json.JSONDecodeError as e:
                print(f"Error decoding JSON in {filename}: {e}")
            except FileNotFoundError:
                print(f"Error: File not found {filename}")
            except Exception as e:
                print(f"Error loading {filename}: {e}")
    return characters

# V2カードが格納されているディレクトリを指定
character_directory = "character"
# キャラクターデータをロード
characters = load_character_cards(character_directory)

# ロードされたキャラクターデータを確認
if characters:
    for character_id, character_data in characters.items():
        print(f"Loaded character: {character_id}")
        #print(character_data) #デバッグ用：キャラクターデータ全体を表示
else:
    print("No character cards found.")

